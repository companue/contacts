{
    "name": "companue/package-skeleton",
    "description": "This is a laravel package for ...",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mimalefdal",
            "email": "mimalefdal@tarasheafzar.ir"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Companue\\PackageSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Companue\\PackageSkeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Companue\\PackageSkeleton\\Providers\\PackageServiceProvider",
                "Companue\\PackageSkeleton\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "PackageSkeleton": "Companue\\PackageSkeleton\\Facades\\PackageSkeleton"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}