{
    "name": "companue/contacts",
    "description": "Laravel package for managing contacts including customers, vendors, and other contact types with multilingual support",
    "keywords": ["laravel", "contacts", "customers", "vendors", "companue", "multilingual"],
    "homepage": "https://github.com/companue/contacts",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mimalefdal",
            "email": "mimalefdal@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Companue\\Contacts\\": "src/",
            "Companue\\Contacts\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Companue\\Contacts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Companue\\Contacts\\Providers\\PackageServiceProvider",
                "Companue\\Contacts\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "Contacts": "Companue\\Contacts\\Facades\\Contacts"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}